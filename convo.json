};



const initializePlayers = (firstVideoId, secondVideoId) => {

player1 = new YT.Player('video-player-1', { height: '100%', width: '100%', videoId: firstVideoId, playerVars: { 'autoplay': 1, 'rel': 0 }, events: { 'onStateChange': onPlayerStateChange } });

player2 = new YT.Player('video-player-2', { height: '100%', width: '100%', videoId: secondVideoId, playerVars: { 'rel': 0 }, events: { 'onStateChange': onPlayerStateChange } });

};



const closeLightbox = () => {

lightbox.classList.remove('is-visible');

if (player1) player1.destroy();

if (player2) player2.destroy();

player1 = null; player2 = null;

lightboxCarousel.innerHTML = '';

document.body.style.overflow = 'auto';

};



