    .buynow-button:hover .buynow-text { transform: scale(1.05) scaleX(0.85); }
    .diagnostic-output { font-family: monospace; color: #ff6b6b; background: #1a0000; border: 1px solid #ff6b6b; padding: 1rem; margin: 2rem auto; max-width: 900px; text-align: left; white-space: pre-wrap; }
