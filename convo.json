<script>
document.addEventListener('DOMContentLoaded', () => {
    const container = document.getElementById('reduce-motion-container');
    const toggle = document.getElementById('reduce-motion-toggle');
    const label = document.getElementById('reduce-motion-label');
    const body = document.body;
    let isInteracting = false;
    let ticking = false;
    let lastScrollY = window.scrollY;

    if (container && toggle && body && label) {
        const updateVisibility = () => {
            const currentScrollY = window.scrollY;
            if (!isInteracting) {
                if (currentScrollY > lastScrollY && currentScrollY > 150) {
                    if (!container.classList.contains('hidden')) {
                        container.classList.add('hidden');
                        window.dispatchEvent(new Event('fixed_elements_update'));
                    }
                } else if (currentScrollY < lastScrollY || currentScrollY <= 50) {
                    if (container.classList.contains('hidden')) {
                        container.classList.remove('hidden');
                        window.dispatchEvent(new Event('fixed_elements_update'));
                    }
                }
            }
            lastScrollY = currentScrollY;
            ticking = false;
        };
        const onScroll = () => { if (!ticking) { window.requestAnimationFrame(updateVisibility); ticking = true; } };
        const handleInteractionStart = () => { isInteracting = true; container.classList.remove('hidden'); };
        const handleInteractionEnd = () => { isInteracting = false; };
        
        window.addEventListener('scroll', onScroll, { passive: true });
        container.addEventListener('mouseenter', handleInteractionStart);
        container.addEventListener('touchstart', handleInteractionStart, { passive: true });
        container.addEventListener('mouseleave', handleInteractionEnd);
        container.addEventListener('touchend', handleInteractionEnd);
        
        const toggleMotion = () => {
            const isActive = toggle.classList.toggle('active');
            body.classList.toggle('no-motion', isActive);
            localStorage.setItem('reduceMotion', isActive ? 'on' : 'off');
        };
        toggle.addEventListener('click', toggleMotion);
        label.addEventListener('click', () => toggle.click());
        
        const savedState = localStorage.getItem('reduceMotion');
        if (savedState === 'on') {
            body.classList.add('no-motion');
            toggle.classList.add('active');
        }
    }
});
</script>
