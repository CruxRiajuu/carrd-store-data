    .product-details .description { font-size: 1rem; color: #aaa; margin-bottom: 2rem; line-height: 1.6; flex-grow: 1; }
    .option-group { border: none; margin-bottom: 1.5rem; padding: 0; }
    .option-group legend { font-weight: 700; font-size: 1rem; text-transform: uppercase; color: #999; margin-bottom: 1rem; letter-spacing: 0.5px; }
    .option-group .option-label { display: inline-block; background-color: #1a1a1a; border: 2px solid #444; color: #ccc; padding: 0.75rem 1.5rem; margin-right: 0.5rem; margin-bottom: 0.5rem; cursor: pointer; transition: all 0.2s ease; user-select: none; border-radius: 4px; box-shadow: inset 0 1px 3px rgba(0,0,0,0.5); }
    .option-group input[type="radio"] { display: none; }
    .option-group input[type="radio"]:checked + .option-label { border-color: #00b9e5; background-color: #013a4a; color: #fff; box-shadow: 0 0 10px rgba(0, 185, 229, 0.5); }
    .option-group .option-label:hover { border-color: #00b9e5; }
    .hidden-options { display: none; }
    .button-wrapper { display: flex; flex-direction: column; gap: 0.75rem; margin-top: auto; }
    @media (min-width: 480px) { .button-wrapper { flex-direction: row; } }
    .buynow-container { flex: 1; min-width: 150px; min-height: 48px; }
    .buynow-button { position: relative; background-color: rgb(125, 187, 206); background-image: linear-gradient(to top, rgba(0, 0, 0, 0.4) 15%, rgb(125, 187, 206)); color: rgb(0, 0, 0); text-decoration: none !important; border-left: 4px solid rgb(66, 155, 182); border-right: 4px solid rgb(66, 155, 182); box-sizing: border-box; display: flex; align-items: center; justify-content: center; width: 100%; height: 48px; transition: all 0.2s ease; box-shadow: 0 4px 8px rgba(0, 0, 0, 0.25); }
    .buynow-button:before, .buynow-button:after { content: ""; position: absolute; top: 0; height: 100%; width: 50%; transition: opacity 0.3s cubic-bezier(0.4, 0, 0.2, 1); opacity: 0; z-index: 0; }
    .buynow-button:before { left: 0; background-image: linear-gradient(to right, #255564, transparent); }
    .buynow-button:after { right: 0; background-image: linear-gradient(to left, #255564, transparent); }
    .buynow-button:hover { background-color: rgb(165, 217, 236); }
