if (playingPlayerId === 'video-player-1' && player2) player2.pauseVideo();

else if (playingPlayerId === 'video-player-2' && player1) player1.pauseVideo();

}

}



function initLightboxController() {

const lightbox = document.getElementById('video-gallery-lightbox');

if (!lightbox) {

setTimeout(initLightboxController, 100);

return;

}



const closeBtn = lightbox.querySelector('.lightbox-close-btn');

const lightboxCarousel = lightbox.querySelector('.lightbox-carousel');



const openLightbox = (clickedVideoId, otherVideoId) => {

lightboxCarousel.innerHTML = `<div class="video-container"><div id="video-player-1"></div></div><div class="video-container"><div id="video-player-2"></div></div>`;

lightbox.classList.add('is-visible');

