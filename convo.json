<script>

document.addEventListener('DOMContentLoaded', function () {



// --- 1. CONFIGURATION ---

const STRIPE_PUBLISHABLE_KEY = 'pk_live_51OHfwcIaRpVk2G5N78UiDGdQLFzmUh1dv6bbA0D4N4I5bK2n7mPruCL8YpY10RuhIImCOJka8aikchX9RAi017k100KJUWJMK7';

// IMPORTANT: Make sure these URLs are the "Raw" GitHub links

const DIGITAL_PRODUCTS_URL = `https://raw.githubusercontent.com/CruxRiajuu/carrd-store-data/main/digital_products.json`;

const PHYSICAL_PRODUCTS_URL = `https://raw.githubusercontent.com/CruxRiajuu/carrd-store-data/main/physical_products.json`;



// --- 2. GLOBAL VARIABLES & ELEMENT SELECTORS ---

let stripe, cart = [], allProducts = [], baseProducts = [];

const cartModal = document.getElementById('cart-modal');

const cartItemsContainer = document.getElementById('cart-items');

const cartTotalEl = document.getElementById('cart-total');

const emptyCartMessage = document.getElementById('empty-cart-message');

const checkoutButton = document.getElementById('checkout-button');

const notificationContainer = document.getElementById('notification-container');

const cartItemCount = document.getElementById('cart-item-count');

const productGrid = document.getElementById('product-grid');

const upsellOverlay = document.getElementById('upsell-modal-overlay');

const upsellAcceptBtn = document.getElementById('upsell-accept');

const upsellDeclineBtn = document.getElementById('upsell-decline');



// --- 3. CORE SHOPPING CART & UPSELL FUNCTIONS ---



// Main function to add an item to the cart

window.addToCart = function(productId, quantity = 1, isUpsell = false) {

if (allProducts.length === 0) return alert("Store data is still loading. Please wait a moment.");

const product = allProducts.find(p => p.id === productId);

if (!product) return alert(`Error: Product with ID ${productId} not found.`);

if (!product.stripe_price_id || !product.stripe_price_id.startsWith('price_')) return alert(`Error: Product "${product.name}" is not configured for checkout.`);



const existingItem = cart.find(item => item.id === productId);

if (existingItem) {

existingItem.quantity += quantity;

} else {

cart.push({ ...product, quantity });

}

saveCart();

renderCart();

showNotification(product.name);



// Trigger the upsell logic if the added item is a T-Shirt or Hoodie

if (!isUpsell && (product.sku?.includes('-TEE-') || product.sku?.includes('-HOOD-'))) {

setTimeout(() => showUpsellOffer(product.id), 1500);

}

};



// Shows the special offer pop-up

function showUpsellOffer(mainProductId) {

const upsellProductId = 802; // The ID of your special "Add-on Sticker"

const upsellProduct = allProducts.find(p => p.id === upsellProductId);

if (!upsellProduct) return;



upsellOverlay.classList.add('visible');



const acceptHandler = () => {

addToCart(upsellProductId, 1, true); // `true` prevents an infinite upsell loop

cleanup();

};

const declineHandler = () => {

cleanup();

};

const cleanup = () => {

upsellOverlay.classList.remove('visible');

upsellAcceptBtn.removeEventListener('click', acceptHandler);

upsellDeclineBtn.removeEventListener('click', declineHandler);

};



upsellAcceptBtn.addEventListener('click', acceptHandler, { once: true });

upsellDeclineBtn.addEventListener('click', declineHandler, { once: true });

}



// Updates quantity from the cart modal input

window.updateQuantity = function(productId, newQuantity) {

const cartItem = cart.find(item => item.id === productId);

if (cartItem) {

cartItem.quantity = Math.max(0, newQuantity);

if (cartItem.quantity === 0) {

removeFromCart(productId);

} else {

saveCart();

renderCart();

}

}

};



// Removes an item completely from the cart

window.removeFromCart = function(productId) {

cart = cart.filter(item => item.id !== productId);

saveCart();

renderCart();

};



// Opens the cart modal

window.openCartModal = function() {

renderCart();

cartModal.classList.remove('hidden');

};



// Closes the cart modal

window.closeCartModal = function() {

const cartContainer = document.getElementById('cart-container');

cartContainer.style.transform = "scale(0.95)";

setTimeout(() => {

cartModal.classList.add('hidden');

cartContainer.style.transform = "";

}, 300);

};



// Handles the checkout process with Stripe

window.goToCheckout = async function() {

const user = firebase.auth().currentUser;

if (!user) {

alert("Please sign in or create an account to proceed to checkout.");

if(typeof openLoginModal === 'function') openLoginModal();

return;

}



if (!stripe || cart.length === 0) return;



const lineItems = cart.map(item => ({ price: String(item.stripe_price_id), quantity: item.quantity }));

const hasPhysicalItems = cart.some(item => item.type === 'physical');



const checkoutOptions = {

lineItems: lineItems,

mode: 'payment',

successUrl: `${window.location.origin}${window.location.pathname}?success=true&session_id={CHECKOUT_SESSION_ID}`,

cancelUrl: window.location.href,

customerEmail: user.email,

clientReferenceId: user.uid

};



if (hasPhysicalItems) {

checkoutOptions.shippingAddressCollection = { allowedCountries: ['US', 'CA', 'GB'] }; // IMPORTANT: Update with countries you ship to

// IMPORTANT: Replace this with your actual Shipping Rate ID from Stripe

checkoutOptions.shipping_options = [{ shipping_rate: 'shr_REPLACE_WITH_YOUR_SHIPPING_RATE_ID' }];

}



try {

checkoutButton.textContent = "Redirecting...";

checkoutButton.disabled = true;

const { error } = await stripe.redirectToCheckout(checkoutOptions);

if (error) {

alert(`An error occurred: ${error.message}`);

checkoutButton.textContent = "Checkout";

checkoutButton.disabled = false;

}

} catch (err) {

alert(`A critical error occurred: ${err.message}`);

checkoutButton.textContent = "Checkout";

checkoutButton.disabled = false;

}

};



// Helper functions

function saveCart() { localStorage.setItem("carrd-cart-v3", JSON.stringify(cart)); }

function loadCart() { const savedCart = localStorage.getItem("carrd-cart-v3"); cart = savedCart ? JSON.parse(savedCart) : []; }

function formatPrice(priceInCents) { return (priceInCents / 100).toLocaleString("en-US", { style: "currency", currency: "USD" }); }



// Updates the number on the cart icon in your navbar

function updateCartCounter() {

const totalItems = cart.reduce((sum, item) => sum + item.quantity, 0);

if (cartItemCount) {

if (totalItems > 0) {

cartItemCount.textContent = totalItems;

cartItemCount.classList.add('visible');

} else {

cartItemCount.classList.remove('visible');

}

}

}



// Shows the "Added to Cart" notification

function showNotification(productName) {

const toast = document.createElement("div");

toast.className = "notification-toast";

toast.innerHTML = `<div class="toast-product-name">${productName}</div><div class="toast-main-message">Added to Cart</div>`;

notificationContainer.appendChild(toast);

setTimeout(() => toast.classList.add("show"), 10);

setTimeout(() => {

toast.classList.remove("show");

toast.addEventListener("transitionend", () => toast.remove());

}, 3000);

}



// Redraws the contents of the cart modal

function renderCart() {

if(!cartItemsContainer) return;

cartItemsContainer.innerHTML = "";

if (cart.length === 0) {

emptyCartMessage.style.display = "block";

checkoutButton.disabled = true;

} else {

emptyCartMessage.style.display = "none";

checkoutButton.disabled = false;

cart.forEach(item => {

const itemEl = document.createElement("div");

itemEl.className = "cart-item";

const imageHtml = item.image ? `<img src="${item.image}" alt="${item.name}">` : '';

itemEl.innerHTML = `

<div class="cart-item-info">${imageHtml}<div class="cart-item-details"><p>${item.name}</p><p>${formatPrice(item.price)}</p></div></div>

<div class="cart-item-quantity">

<input type="number" value="${item.quantity}" min="0" onchange="updateQuantity(${item.id}, parseInt(this.value))">

<button class="cart-item-remove" onclick="removeFromCart(${item.id})"><svg fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"></path></svg></button>

</div>`;

cartItemsContainer.appendChild(itemEl);

});

}

const subtotal = cart.reduce((sum, item) => sum + item.price * item.quantity, 0);

cartTotalEl.textContent = formatPrice(subtotal);

updateCartCounter();

}



// --- 4. PRODUCT RENDERING & INITIALIZATION ---



// Creates the product cards on your page

function renderProducts() {

if (!productGrid) return;

productGrid.innerHTML = '';



baseProducts.forEach(product => {

if (product.hidden) return; // This will hide the "Add-on Sticker" from the main grid



const card = document.createElement('div');

card.className = 'product-card';



if (product.variants && product.variants.length > 1) { // Renders dropdown for products with multiple variants

const variants = product.variants;

let optionsHTML = variants.map(v => `<option value="${v.id}">${v.name.replace(product.base_name, '').trim() || product.name} - ${formatPrice(v.price)}</option>`).join('');



card.innerHTML = `

<img src="${product.base_image}" alt="${product.base_name}" class="base-image">

<h3>${product.base_name}</h3>

<p>${product.description}</p>

<select id="select-${product.base_id}" onchange="updateVariantPrice('${product.base_id}')">${optionsHTML}</select>

<div class="price-display" id="price-${product.base_id}">${formatPrice(variants[0].price)}</div>

<div class="buynow-container" style="margin-top: auto;">

<a href="javascript:void(0);" onclick="addToCart(parseInt(document.getElementById('select-${product.base_id}').value))" class="buynow-button"><span class="buynow-text">Add to Cart</span></a>

</div>`;

} else { // Renders a simple card for products with one or no variants

const singleProduct = product.variants ? product.variants[0] : product;

card.innerHTML = `

<img src="${singleProduct.image || product.base_image || 'https://placehold.co/400x400/0c0c0c/fff?text=Art'}" alt="${singleProduct.name}" class="base-image">

<h3>${singleProduct.name}</h3>

<p>${product.description || ''}</p>

<div class="price-display">${formatPrice(singleProduct.price)}</div>

<div class="buynow-container" style="margin-top: auto;">

<a href="javascript:void(0);" onclick="addToCart(${singleProduct.id})" class="buynow-button"><span class="buynow-text">Add to Cart</span></a>

</div>`;

}

productGrid.appendChild(card);

});

}



// Updates the price display when a variant is selected

window.updateVariantPrice = (baseId) => {

const selectEl = document.getElementById(`select-${baseId}`);

const priceEl = document.getElementById(`price-${baseId}`);

const selectedId = parseInt(selectEl.value);

const product = allProducts.find(p => p.id === selectedId);

if (product) {

priceEl.textContent = formatPrice(product.price);

}

};



// Main function to fetch products and start the store

async function initializeStore() {

try {

