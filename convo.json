} else {

renderError(`PRODUCT NOT FOUND. Could not find a product with base_id: "${productIdFromUrl}" in your JSON files.`);

}

} catch (error) {

console.error("Failed to initialize product page:", error);

renderError("ERROR: Could not load product data from GitHub. Check the browser console (F12) for details.");

}

}



initializePage();

window.addEventListener('hashchange', initializePage);

});

