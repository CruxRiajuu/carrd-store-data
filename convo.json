// REPLACE your old goToCheckout function with this one.

window.goToCheckout = async function() {
    // Check if the user is logged in
    if (typeof firebase === 'undefined' || !firebase.auth().currentUser) {
        alert("Please sign in or create an account to proceed to checkout.");
        if(typeof openLoginModal === 'function') openLoginModal();
        return;
    }
    const user = firebase.auth().currentUser;

    // Check if Stripe and the cart are ready
    if (!stripe || cart.length === 0) return;

    // Prepare the items for Stripe
    const lineItems = cart.map(item => ({
        price: String(item.stripe_price_id),
        quantity: item.quantity
    }));

    // Check if there are any physical items that require shipping
    const hasPhysicalItems = cart.some(item => item.type === 'physical');
    
    // --- THIS IS THE NEW LOGIC ---
    const checkoutOptions = {
        lineItems: lineItems,
        mode: 'payment',
        successUrl: `${window.location.origin}${window.location.pathname}?success=true&session_id={CHECKOUT_SESSION_ID}`,
        cancelUrl: window.location.href,
        customerEmail: user.email,
        clientReferenceId: user.uid
    };

    if (hasPhysicalItems) {
        checkoutOptions.shippingAddressCollection = {
            allowedCountries: ['US', 'CA', 'GB', 'AU'] // Add all countries you ship to
        };

        // Provide ALL your shipping rates to Stripe.
        // Stripe will automatically show the correct ones based on the customer's address.
        checkoutOptions.shipping_options = [
