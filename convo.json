if (event.data && event.data.type === 'open-video-lightbox') {

openLightbox(event.data.clickedId, event.data.otherId);

}

});



closeBtn.addEventListener('click', closeLightbox);

lightbox.addEventListener('click', (e) => {

if (e.target === lightbox) closeLightbox();

});

}



// Use the YouTube API's ready function to initialize our listener

window.onYouTubeIframeAPIReady = function() {

